{
  "name": "sf-zsi",
  "displayName": "sf zsi",
  "repository": "https://github.com/codythomaszeitler/sf-zsi.git",
  "description": "vscode extensions for salesforce.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:sfdx-project.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sf.zsi.openOrg",
        "title": "SF ZSI: Open Org"
      },
      {
        "command": "sf.zsi.projectDeploy",
        "title": "SF ZSI: Project Deploy"
      },
      {
        "command": "sf.zsi.generateFauxSObjects",
        "title": "SF ZSI: Generate Faux SObjects"
      },
      {
        "command": "sf.zsi.runHighlightedApex",
        "title": "SF ZSI: Run Highlighted Apex"
      },
      {
        "command": "sf.zsi.enableDebugLogForCurrentUser",
        "title": "SF ZSI: Enable Debug Logging"
      },
      {
        "command": "sf.zsi.getRecentApexLogs",
        "title": "SF ZSI: Get Recent Apex Logs"
      },
      {
        "command": "sf.zsi.cleanLocalApexLogs",
        "title": "SF ZSI: Clean Local Apex Logs"
      },
      {
        "command": "sf.zsi.refreshApexLogs",
        "title": "SF ZSI: Refresh Apex Logs"
      },
      {
        "command": "sf.zsi.runTestUnderCursor",
        "title": "SF ZSI: Run Test Under Cursor"
      }
    ],
    "views": {
      "test": [
        {
          "title": "Apex Logs",
          "id": "server-side-apex-logs",
          "name": "Apex Logs",
          "icon": "media/dep.svg"
        }
      ]
    },
    "configuration": {
      "title": "sf zsi",
      "properties": {
        "sf.zsi.vscode.shouldFocusProblemsWhenDeployFails": {
          "type": "boolean",
          "default": true,
          "description": "It 'Project Deploy' should focus the problems tab when a failure occurs."
        },
        "sf.zsi.vscode.proxy": {
          "type": "object",
          "description": "Proxy settings given to the shell as a JSON object"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/glob-to-regexp": "^0.4.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "glob-to-regexp": "^0.4.1",
    "jest": "^29.7.0",
    "mocha": "^9.1.3",
    "ts-jest": "^29.1.1",
    "typescript": "^4.4.4"
  }
}
